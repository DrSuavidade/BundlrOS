{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "declaration": true,
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "**/*.tsx", "../env.d.ts"],
  "exclude": ["dist", "node_modules"]
}
